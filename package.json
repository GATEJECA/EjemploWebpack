{
  "name": "ejemplowebpack",
  "version": "1.0.0",
  "description": "Ejemplo de aplicación para ver el funcionamiento de webpack",
  "main": "index.js",
  "scripts": {
    "build": "webpack index.js bundle.js",
    "build:local": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:plugin": "webpack --config ./plugin-extract-text/webpack.config.js",
    "build:multiple": "webpack --config ./multiple-entry-points/webpack.config.js",
    "build:dev": "webpack --config ./webpack-dev-server/webpack.config.js --watch",
    "build:babel": "webpack --config ./babel-loader/webpack.config.js",
    "build:images": "webpack --config ./url-loader-images/webpack.config.js",
    "build:fonts": "webpack --config ./url-loader-fonts/webpack.config.js",
    "build:videos": "webpack --config ./url-loader-videos/webpack.config.js",
    "build:json": "webpack --config ./url-loader-json/webpack.config.js",
    "build:react": "webpack --config ./react/webpack.config.js",
    "build:sass": "webpack --config ./sass-loader/webpack.config.js",
    "build:stylus": "webpack --config ./stylus-loader/webpack.config.js",
    "build:less": "webpack --config ./less-loader/webpack.config.js",
    "build:desarrollo": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js"
  },
  "keywords": [
    "Webpack"
  ],
  "author": "Carlos García @GATEJECA",
  "license": "MIT",
  "devDependencies": {
    "ajv": "^6.0.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.10",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "json-loader": "^0.5.7",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "nib": "^1.1.2",
    "node-sass": "^4.0.0",
    "rupture": "^0.7.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.0.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
